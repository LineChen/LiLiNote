<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="2px"
                android:color="@color/line_divider_color" />
        </shape>
    </item>

    <item
        android:bottom="2px"
        android:left="0dp"
        android:right="0dp"
        android:top="0px">

        <shape android:shape="rectangle">
            <solid android:color="@color/md_grey_100" />
        </shape>
    </item>
</layer-list>